<xsd:element name="monitoringGroup" type="tMonitoringGroup"/>

<xsd:complexType name="tMonitoringGroup">
<xsd:complexContent>
	<xsd:extension base="tArtifact">
		<xsd:sequence>
			<xsd:element ref="ioSpecification" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="dataOutputAssociation" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="monitoringVariable" type="tMonitoringVariable" minOccurs="1" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="description" type="xsd:string"/>
	</xsd:extension>
</xsd:complexContent>
</xsd:complexType>

<xsd:complexType name="tMonitoringVariable">
<xsd:complexContent>
	<xsd:extension base="tBaseElement">
		<xsd:attribute name="varName" type="xsd:string"/>
		<xsd:attribute name="description" type="xsd:string"/>
		<xsd:attribute name="type" type="xsd:string" default="xsd:string"/>
		<xsd:attribute name="required" type="xsd:boolean" default="false"/>
		<xsd:attribute name="changeability" type="tChangeabilityTypes" default="static"/>
	</xsd:extension>
</xsd:complexContent>
</xsd:complexType>

<xsd:simpleType name="tChangeabilityTypes">
	<xsd:restriction base="xsd:string">
		<xsd:enumeration value="static"/>
		<xsd:enumeration value="dynamic"/>
	</xsd:restriction>
</xsd:simpleType>




<xsd:element name="activityEffect" type="tActivityEffect"/>

<xsd:complexType name="tActivityEffect">
<xsd:complexContent>
	<xsd:extension base="tArtifact">
		<xsd:sequence>
			<xsd:element name="activityEffectRule" type="tActivityEffectRule" minOccurs="1" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="description" type="xsd:string"/>
	</xsd:extension>
</xsd:complexContent>
</xsd:complexType>

<xsd:complexType name="tActivityEffectRule">
<xsd:complexContent>
	<xsd:extension base="tBaseElement">
		<xsd:attribute name="description" type="xsd:string"/>
		<xsd:attribute name="rule" type="xsd:string"/>
	</xsd:extension>
</xsd:complexContent>
</xsd:complexType>




<xsd:element name="timeExpected" type="tTimeExpected"/>

<xsd:complexType name="tTimeExpected">
<xsd:complexContent>
	<xsd:extension base="tArtifact">
		<xsd:sequence>
			<xsd:element name="expected" type="tTimeValue" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="margin" type="tTimeValue" minOccurs="1" maxOccurs="1"/>
		</xsd:sequence>
		<xsd:attribute name="description" type="xsd:string"/>
	</xsd:extension>
</xsd:complexContent>
</xsd:complexType>

<xsd:complexType name="tTimeValue">
<xsd:complexContent>
	<xsd:extension base="tBaseElement">
		<xsd:attribute name="value" type="xsd:decimal" default="0.0"/>
		<xsd:attribute name="unit" type="xsd:string" default="s"/>
		<xsd:attribute name="description" type="xsd:string"/>
	</xsd:extension>
</xsd:complexContent>
</xsd:complexType>


<xsd:element name="decisionQuestion" type="tDecisionQuestion"/>

<xsd:complexType name="tDecisionQuestion">
<xsd:complexContent>
	<xsd:extension base="tArtifact">
		<xsd:sequence>
			<xsd:element ref="ioSpecification" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="dataInputAssociation" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
		<xsd:attribute name="description" type="xsd:string"/>
 		<xsd:attribute name="expression" type="xsd:string"/>
		<xsd:attribute name="type" type="tDecisionQuestionTypes"/>
	</xsd:extension>
</xsd:complexContent>
</xsd:complexType>

	<xsd:simpleType name="tDecisionQuestionTypes">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="normal"/>
			<xsd:enumeration value="quality"/>
		</xsd:restriction>
	</xsd:simpleType>

